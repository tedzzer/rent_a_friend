


<div class="container">
  <%= simple_form_for(@user, url: user_session_path) do |f| %>
    <%= f.input :name %>
    <%= f.input :age %>
    <%= f.input :personal_description %>
    <%= f.input :location %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :password_confirmation %>
    <%= f.button :submit %>
  <% end %>

</div>

<%= render "devise/shared/links" %>
